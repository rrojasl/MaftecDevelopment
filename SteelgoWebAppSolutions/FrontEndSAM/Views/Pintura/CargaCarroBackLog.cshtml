@{
    ViewBag.Title = "Carga de carro";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="lblInicio"></span></a>
    </li>
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="">Pintura</span></a>
    </li>
    <li class="active">
        <a href="@Url.Action("CapturaArmado", "Armado")"><span id="">Carga de carro</span></a>
    </li>
}


<div id="FormListadoRequisicion" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">

    <div class="row addedSection">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div id="containerDiv">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <label id=""><span id="PinturaCargaBackLogCarro"></span></label>
                            <input id="inputCarro" class="" />
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <br />
                            <button id="btnSubir" type="button" class="btn btn-yellow"><span id="PinturaCargaBackLogBtnSubirCarro"></span></button>
                        </div>
                        <div class="col-sm-3 col-md-3 col-lg-3">
                            <label id="">Cerrar carro</label>
                            <input type="checkbox" name="Cerrar" style=" width:auto;" id="chkCerrar" />
                        </div>
                    </div>
                </div>
                <br /><br />
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <div id="ContenedorGrid" class="row">
                            <div id="grid" data-role="grid" class="k-grid k-widget">
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <input id="hdAccionFiltrado" type="hidden" />
    <div id="window">
    </div>
    <br /><br />

</div>


<script type="text/javascript" src="~/Scripts/AppComponents/Pintura/CargaCarroBackLog/Render.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Pintura/CargaCarroBackLog/Handlers.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Pintura/CargaCarroBackLog/Ajax.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Pintura/CargaCarroBackLog/Captura.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Pintura/CargaCarroBackLog/Seguridad.js"></script>



<script>
    @section JavascriptDocumentReadyFunctions {
    $authorizationModel["CargaCarroBackLog"] = $CargaCarroBackLogModel;
    }
</script>

