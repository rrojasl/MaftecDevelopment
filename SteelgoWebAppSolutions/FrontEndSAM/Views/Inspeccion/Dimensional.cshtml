@{

    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="lblInicio"></span></a>
    </li>
    <li>
        <a href="#"><span id="lblInspeccion"></span></a>
    </li>
    <li class="active">
        <a href="@Url.Action("Dimensional", "Inspeccion")"><span id="DimensionalVisualInspeccionDimensional"></span></a>
    </li>
}
<div id="FormVisualDimensional" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">
    @*Menu superior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow accionGuardar"><span id="botonGuardar"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar" class="accionGuardar"><span id="botonGuardar2"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo"><span id="botonGuardarYNuevo"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar"></span></a>
                </div>
                <div id="RequisicionDiv" class="button-section">
                    <a id="Requisicion" href="@Url.Action("GenerarRequisicion","ServiciosTecnicos")" class="btn btn-primary"><span id="MensajeRequisicion"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones1"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones">
                            @*<li><a id="RedirectDashboard" ><span id="DetalleAvisoLlegada0065"></span></a></li>
                                <li><a><span id="DetalleAvisoLlegada0066"></span></a></li>
                                <li><a id="CancelarFolio" href="#"><span id="DetalleAvisoLlegada0083"></span></a></li>*@
                        </ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
    @*Controles Superior*@
    <div class="row">
        <div class="col-xs-4 col-sm-4 col-md-3 col-lg-2" id="InputIDDiv">
            
                <label id="DimensionalVisualHeaderSpoolID"></label>
                <input style="width:40%" id="InputOrdenTrabajo" class="item-select" />-<input id="InputID" placeholder="ID" style="width:40%;" />
            
            <input type="hidden" id="InspeccionDimensionalID" />
        </div>
        <div class="col-xs-4 col-sm-4 col-md-3 col-lg-2">
            <label id="">&nbsp;</label>
            <button type="button" id="btnAgregar" class="btn btn-blue"><span id="botonAgregar"></span></button>
        </div>
        <div class="col-xs-4 col-sm-4 col-md-6 col-lg-10"></div>
    </div>
    @*GRID*@
    <div class="row addedSectionInLine">
        <div class="col-sm-6 col-md-4 col-lg-2">
            <label id="DimensionalVisualHeaderResultadoDimesional"></label>
            <input type="radio" name="ResultadoDimensional" id="ResultadoDimensionalAprobado" style=" width:auto;" value="Aprobado" /><span id="DimensionalVisualAporbadoDimensional"></span>
            <input type="radio" name="ResultadoDimensional" id="ResultadoDimensionalRechazado" style=" width:auto;" value="Rechazado" /><span id="DimensionalVisualRechazadoDimensional"> </span>
        </div>
        <div class="col-sm-6 col-md-4 col-lg-2" id="inputDefectoDiv">
            <label id="DimensionalVisualHeaderDefectoDimesional"></label>
            <input id="inputDefecto" class="" />
        </div>
        <div class="col-sm-6 col-md-4 col-lg-2" id="inputInspectorDiv">
            <label id="DimensionalVisualHeaderInspectorDimesional"></label>
            <input id="inputInspector" class="" />
        </div>
        <div class="col-sm-6 col-md-4 col-lg-2" id="FechaInspeccionDiv">
            <label id="DimensionalVisualHeaderFechaDimesional"></label>
            <input id="FechaInspeccion" />
        </div>
        <div class="col-sm-6 col-md-4 col-lg-2">
            <label id="CapturaLlenadoMasivo"></label>
            <input type="radio" name="LLena" style=" width:auto" value="Todos" /><span id="CapturaLlenadoMasivoTodos"></span>
            <input type="radio" name="LLena" style=" width:auto" value="Vacios" /><span id="CapturaLlenadoMasivoVacios"></span>
        </div>
        <div class="col-sm-6 col-md-4 col-lg-2">
            <label id="">&nbsp;</label>
            <button type="button" id="btnAplicarCapturaRapida" class="btn btn-blue"><span id="botonAplicar"></span></button>
        </div>

        <div id="ContenedorGrid" class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div id="grid" data-role="grid" class="k-grid k-widget">
            </div>
        </div>
        <input id="hdAccionFiltrado" type="hidden" />
    </div>
    @*Menu Inferior*@
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div class="formNav clearfix">
                <div class="button-section editButtonSection">
                    <div class="btn-group save-group">
                        <button id="Guardar1" onclick="javascript:void(0);" type="button" class="btn btn-yellow accionGuardar"><span id="botonGuardar3"></span></button>
                        <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="caretWhite"></span>
                        </button>
                        <ul class="dropdown-menu">
                            <li><a href="#" id="btnGuardar1" class="accionGuardar"><span id="botonGuardar4"></span></a></li>
                            <li><a href="#" id="btnGuardarYNuevo1"><span id="botonGuardarYNuevo1"></span></a></li>
                        </ul>
                    </div>
                    <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar1"></span></a>
                </div>
                <div id="RequisicionDiv" class="button-section">
                    <a id="Requisicion" href=@Url.Action("GenerarRequisicion","ServiciosTecnicos") class="btn btn-primary"><span id="MensajeRequisicion1"></span></a>
                </div>
                <div class="button-section actionButtonSection">
                    <div class="dropdown action-group">
                        <a id="Acciones" data-target="#" href="#" data-toggle="dropdown" role="button" ariahaspopup="true" aria-expanded="false" class="btn btn-default">
                            <span id="botonAcciones2"></span>
                            <span class="caretBlue"></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="Acciones">
                            @*<li><a id="RedirectDashboard" ><span id="DetalleAvisoLlegada0065"></span></a></li>
                                <li><a><span id="DetalleAvisoLlegada0066"></span></a></li>
                                <li><a id="CancelarFolio" href="#"><span id="DetalleAvisoLlegada0083"></span></a></li>*@
                        </ul>
                    </div>
                </div>
                <a id="Imprimir" href="#" class="btn btn-fadeBlue actionButtonSection"></a>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript" src="~/Scripts/AppComponents/Inspeccion/Dimensional/Ajax.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Inspeccion/Dimensional/Handlers.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Inspeccion/Dimensional/Render.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Inspeccion/Dimensional/Captura.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Inspeccion/Dimensional/Seguridad.js"></script>

<script>
    @section JavascriptDocumentReadyFunctions {
    $authorizationModel["Dimensional"] = $DimensionalModel;
    }
</script>