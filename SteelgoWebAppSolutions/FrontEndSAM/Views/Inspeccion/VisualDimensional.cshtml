@{
    ViewBag.Title = "Visual Dimensional";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="ListadoLlegadaMaterial0020"></span></a>
    </li>

    <li class="active">
        <a href="@Url.Action("", "")"><span id="">Inspeccion visual dimensional</span></a>
    </li>
}
<div id="FormVisualDimensional" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12">
            <div class="button-section editButtonSection">
                <div class="btn-group save-group">
                    <button id="Guardar" onclick="javascript:void(0);" type="button" class="btn btn-yellow"><span id="botonGuardar2"></span></button>
                  
                    <button id="dropdown-toggle" type="button" class="btn btn-yellow dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <span class="caretWhite"></span>
                    </button>
                    <ul class="dropdown-menu">
                        <li><a href="#" id="btnGuardar"><span id="botonGuardar"></span></a></li>
                        <li><a href="#" id="btnGuardarYNuevo"><span id="botonGuardarYNuevo"></span></a></li>
                    </ul>
                </div>
                <a id="btnCancelar" class="btn btn-black"><span id="botonCancelar"></span></a>
            </div>
        </div>
    </div>
    <hr />
    <div class="row">
        <div class="col-sm-2 col-md-2 col-lg-2">
            <label id="DimensionalVisualHeaderSpoolID"></label>
            <input style="width:40%" id="InputOrdenTrabajo" class="item-select" />-<input id="InputID" placeholder="ID" style="width:40%;" />
            <input type="hidden" id="InspeccionDimensionalID" />
        </div>
        <div class="col-sm-3 col-md-3 col-lg-3">
            <label id="DimensionalVisualHeaderResultadoDimesional"></label>
            <div class="btn-group" data-toggle="buttons">
                <label class="btn btn-default" id="StyleResultadoDimensionalA">
                    <input type="radio" name="ResultadoDimensional" id="ResultadoDimensional" value="Aprobado" /><span id="DimensionalVisualAporbadoDimensional"> </span>
                </label>
                <label class="btn btn-default" id="StyleResultadoDimensionalR">
                    <input type="radio" name="ResultadoDimensional" value="Rechazdo" /><span id="DimensionalVisualRechazadoDimensional"> </span>
                </label>
            </div>
        </div>
        <div class="col-sm-2 col-md-2 col-lg-2">
            <label id="DimensionalVisualHeaderDefectoDimesional"></label>
            <input id="inputDefecto" class="" />
        </div>
        <div class="col-sm-2 col-md-2 col-lg-2">
            <label id="DimensionalVisualHeaderInspectorDimesional"></label>
            <input id="inputInspector" class="" />
        </div>
        <div class="col-sm-2 col-md-2 col-lg-2">
            <label id="DimensionalVisualHeaderFechaDimesional"></label>
            <input id="FechaInspeccion" />
        </div>
        <div class="col-sm-1 col-md-1 col-lg-1">
            <label id="">&nbsp;</label>
            <button type="button" id="btnAgregar" class="btn btn-blue"><span id="botonAgregar"></span></button>
        </div>
    </div>
    <hr />
    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12">
            <div class="row">
                <div class="col-sm-4 col-md-4 col-lg-4">
                    <label id="DimensionalVisualHeadeTaller"></label>
                    <input id="inputTaller" class="" />
                </div>
                <div class="col-sm-4 col-md-4 col-lg-4">
                    <label id="DimensionalVisualHeaderResultadoVisual"></label>
                    <div class="btn-group" data-toggle="buttons">
                        <label class="btn btn-default" id="StyleResultadoVisualA">
                            <input type="radio" name="ResultadoVisual" id="ResultadoVisual" style=" width:auto;" value="Aprobado" /><span id="DimensionalVisualAporbadoVisual"> </span>
                        </label>
                        <label class="btn btn-default" id="StyleResultadoVisualR">
                            <input type="radio" name="ResultadoVisual" style=" width:auto;" value="Rechazdo" /><span id="DimensionalVisualRechazadoVisual"> </span>
                        </label>
                    </div>
                </div>
                <div class="col-sm-4 col-md-4 col-lg-4">
                    <label id="DimensionalVisualHeaderDefectoVisual"></label>
                    <input id="inputDefectosVisual" class="" />
                </div>
            </div>
            <div class="row">
                <div class="col-sm-4 col-md-4 col-lg-4">
                    <label id="DimensionalVisualHeaderInspectorVisual"></label>
                    <input id="inputInspectorVisual" class="" />
                </div>
                <div class="col-sm-4 col-md-4 col-lg-4">
                    <label id="DimensionalVisualHeaderFechaVisual"></label>
                    <input id="inputFechaVisual" class="" />
                </div>
                <div class="col-sm-4 col-md-4 col-lg-4">
                    <label id="CapturaArmadoHeaderLlenadoMasivo"></label>
                    <div class="btn-group" data-toggle="buttons">
                        <label class="btn btn-default" id="StylePlanchaTodos">
                            <input type="radio" name="LLena" value="Todos" /><span id="CapturaArmadoTodos2"></span>
                        </label>
                        <label class="btn btn-default" id="StylePlanchaVacios">
                            <input type="radio" name="LLena" value="Vacios" /><span id="CapturaArmadoVacios"></span>
                        </label>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <div id="ContenedorGrid" class="row">
                        <div id="grid" data-role="grid" class="k-grid k-widget">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="window">
    </div>
    <input id="hdAccionFiltrado" type="hidden" />
</div>
<script type="text/javascript" src="~/Scripts/AppComponents/Inspeccion/Ajax.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Inspeccion/Handlers.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Inspeccion/Render.js"></script>
<script type="text/javascript" src="~/Scripts/AppComponents/Inspeccion/Captura.js"></script>

