@{
    ViewBag.Title = "Listado Requisición";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="lblInicio"></span></a>
    </li>
    <li class="active">
        <a href="@Url.Action("CapturaArmado", "Armado")"><span id="">Listado Requisicion</span></a>
    </li>
}

<div></div>


<div id="FormListadoRequisicion" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">

    <div class="row">
        <div class="col-sm-12 col-md-12 col-lg-12">
            <div class="button-section editButtonSection">
                <a id="btnCancelar" class="btn btn-yellow"><span id="">Agregar Requisicion</span></a>
            </div>
        </div>
    </div>
    <hr />
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
     
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <div class="row">
                            <div class="btn-group tabList col-xs-12 col-sm-12 col-md-12 col-lg-12" role="group" arial-label="tabList">
                               
                                <button id="btnSinPermiso" class="btn btn-tabList"><span id="">Capturado </span><span id="">10</span></button>
                                <button id="btnSinAutorizacion" class="btn btn-tabList active"><span id="">Asignados </span><span id="">10</span>10</button>
                                <button id="btnColetos" class="btn btn-tabList"><span id="">Entregados </span><span id="">10</span></button>
                                <button id="btnCompletos" class="btn btn-tabList"><span id="">Validados </span><span id="">20</span></button>
                            </div>
                        </div>
                        <div id="ContenedorGrid">
                            <div id="grid" data-role="grid" class="k-grid k-widget">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
    </div>
    <input id="hdAccionFiltrado" type="hidden" />
    <div id="window">
    </div>
</div>
