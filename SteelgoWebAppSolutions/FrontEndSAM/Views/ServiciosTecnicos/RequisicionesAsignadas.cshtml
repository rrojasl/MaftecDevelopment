
@section breadcrumb {
    <li>
        <a href="@Url.Action("landing", "Home")"><span id="lblInicio"></span></a>
    </li>
    <li class="active">
        <a href="@Url.Action("CapturaArmado", "Armado")"><span id="">Requisiciones asignadas</span></a>
    </li>
}

<div id="FormRequisicionesasignadas" class="form clearfix col-xs-12 col-sm-12 col-md-12 col-lg-12">

    <div class="row">
        <div class="col-sm-3 col-md-3 col-lg-3">
            <label id="CapturaArmadoHeaderMostrar"></label>
            <div class="btn-group" data-toggle="buttons">
                <label class="btn btn-default" id="styleSinCaptura">
                    <input type="radio" name="Muestra" style=" width:auto;" value="Sin Capturar" /><span id="">Pendientes</span>
                </label>
                <label class="btn btn-default" id="styleTodos">
                    <input type="radio" name="Muestra" style=" width:auto;" value="Todos" /><span id="CapturaArmadoTodos"> </span>
                </label>
            </div>
        </div>



    </div>
    <hr />
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">

            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <div id="ContenedorGrid" class="row">
                        <div id="grid" data-role="grid" class="k-grid k-widget">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <input id="hdAccionFiltrado" type="hidden" />
    <div id="window">
    </div>
</div>
